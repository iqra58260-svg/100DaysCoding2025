//Soal 3
public class Soal3 {

    public static void main(String[] args) {
        
        Scanner input = new Scanner(System.in);
        System.out.print("Batas Bawah : ");
        int a = input.nextInt();
        String b = (a%2 == 0)? "Iyaji":"Tidakji";
        System.out.println(b);
        
    }
    
}
    }
